{
    "name": "shel/neos-logs",
    "type": "neos-plugin",
    "license": "GPL-3.0+",
    "description": "Allows access to logfiles and exceptions from the Neos CMS backend",
    "keywords": [
        "neos",
        "neoscms",
        "flow",
        "logging",
        "logfile",
        "log",
        "exception",
        "viewer",
        "analyse"
    ],
    "require": {
        "php": "^8.1",
        "neos/flow": "^8.3 || ^9.0",
        "neos/neos": "^8.3 || ^9.0"
    },
    "authors": [
        {
            "name": "Sebastian Helzle",
            "email": "sebastian@helzle.it",
            "homepage": "https://www.helzle.it",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shel\\Neos\\Logs\\": "Classes"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Shel.Neos.Logs"
        }
    }
}
